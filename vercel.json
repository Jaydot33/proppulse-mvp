{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "main.py"
    },
    {
      "src": "/dashboard/(.*)",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/dashboard"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.12"
  }
}
